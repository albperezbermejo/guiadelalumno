nav.navbar.navbar-inverse.navbar-static-top(ng-controller='NavbarCtrl')
    .container-fluid
        .navbar-header
            button.navbar-toggle(type='button' data-toggle='collapse' data-target='#topNavbar')
                span.icon-bar
                span.icon-bar
                span.icon-bar
            a.navbar-brand(href='#') U-tad
        #topNavbar.collapse.navbar-collapse
            ul.nav.navbar-nav
                li.dropdown
                    a.dropdown-toggle(data-toggle='dropdown' href='#')
                        | Our asignatures
                        span.caret
                    ul.dropdown-menu
                        li(ng-repeat='item in menu')
                            a(href='{{item.link}}') {{ item.title }}

                li.active
                    a(href='#') FAQ
            ul.nav.navbar-nav.navbar-right
                li
                    a(href='#enrollModal' data-toggle='modal')
                        span.glyphicon.glyphicon-user
                        |  Sign Up
                li
                    a(href='#loginModal' data-toggle='modal')
                        span.glyphicon.glyphicon-log-in
                        |  Login